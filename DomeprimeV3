public class DemoprimeV3{
    public static void main(String args[])
    {
        int num,flag=0;
        num=Integer.parseInt(args[0]);
        for(int i=2;i<=num;i++)
        {
            if(num==0)
            {
                System.out.println(num+ "is not a prime number");
                flag=1;
                break;
            }
        }
        if(flag==0)
        {
            System.out.println(num+"is a prme number");
        }
    }
}
